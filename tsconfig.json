{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "esModuleInterop": true,
    "jsxImportSource": "solid-js",
    "jsx": "preserve",
    "module": "ESNext",
    "moduleResolution": "node",
    "paths": {
      "~/*": ["./src/*"]
    },
    "strict": true,
    "target": "ESNext",
    "types": ["vite/client"]
  }
}
